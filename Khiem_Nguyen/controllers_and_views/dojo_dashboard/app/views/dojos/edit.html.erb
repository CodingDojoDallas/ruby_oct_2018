<h1>Editing <%= @dojo.branch %></h1>

<form action='/dojos/<%= @dojo.id %>' method="post">
    <input type='hidden' name='_method' value='patch'>
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <h4>Branch:<input type="text" name="dojo[branch]" value="<%= @dojo.branch %>"></h4>
    <h4>Branch:<input type="text" name="dojo[street]" value="<%= @dojo.street %>"></h4>
    <h4>Branch:<input type="text" name="dojo[city]" value="<%= @dojo.city %>"></h4>
    <h4>Branch:<input type="text" name="dojo[state]" value="<%= @dojo.state %>"></h4>
    <button type="submit">Update</button>
</form>
<% if flash[:error] %>
    <%= flash[:error] %>
<% end %>

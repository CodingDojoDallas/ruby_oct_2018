<html>
    <head>
        <title>New Dojo</title>
    </head>
    <body>
        <h1>New Dojo</h1>
        <form action="/create" method="POST">
        <input type="hidden"  name="authenticity_token" value="<%= form_authenticity_token %>">
        <table>
            <tr><td>Branch:</td></tr>
            <tr><td><input type="text" name="dojo[branch]"></td></tr>
            <tr><td>Street:</td></tr>
            <tr><td><input type="text" name="dojo[street]"></td></tr>
            <tr><td>City:</td></tr>
            <tr><td><input type="text" name="dojo[city]"></td></tr>
            <tr><td>State:</td></tr>
            <tr><td><input type="text" name="dojo[state]"></td></tr>
        </table>
        <button type="submit">Create</button>
        </form>
        <a href="/">Home</a>
    </body>
</html>